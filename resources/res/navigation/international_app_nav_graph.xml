    </keep_state_fragment>
    <keep_state_fragment android:label="PermissionFragment" android:name="com.xiaopeng.appstore.appstore_ui.fragment.PermissionFragment" android:id="@+id/permissionFragment"/>
    <keep_state_fragment android:label="PrivatePolicyFragment" android:name="com.xiaopeng.appstore.appstore_ui.fragment.PrivatePolicyFragment" android:id="@+id/privatePolicyFragment"/>
    <action android:id="@+id/back_to_home" app:destination="@+id/appListFragment" app:popUpTo="@+id/international_app_list_nav_graph" app:popUpToInclusive="true"/>
