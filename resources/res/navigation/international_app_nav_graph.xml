<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/international_app_list_nav_graph" app:startDestination="@+id/appListFragment">
    <keep_state_fragment android:label="AppListFragment" android:name="com.xiaopeng.appstore.international.list.InternationalAppListFragment" android:id="@+id/appListFragment">
