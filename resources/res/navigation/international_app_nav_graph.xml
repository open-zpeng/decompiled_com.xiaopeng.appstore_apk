        <action android:id="@+id/action_appListFragment_to_itemDetailFragment" app:destination="@+id/itemDetailFragment" app:enterAnim="@anim/nav_enter_anim" app:exitAnim="@anim/nav_exit_anim" app:launchSingleTop="true" app:popEnterAnim="@anim/nav_pop_enter_anim" app:popExitAnim="@anim/nav_pop_exit_anim"/>
